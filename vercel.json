{
  "version": 2,
  "builds": [
    {
      "src": "api/get-ndvi.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/get-ndvi",
      "dest": "/api/get-ndvi.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}

